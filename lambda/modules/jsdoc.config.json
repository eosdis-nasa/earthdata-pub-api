{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./action-handler/src",
      "./notification-handler/src",
      "./workflow-handler/src",
      "./dashboard/src",
      "./information/src",
      "./invoke/src",
      "./notify/src",
      "./register/src",
      "./submission/src",
      "./subscription/src",
      "./actions",
      "./database-driver/src",
      "./message-driver/src",
      "./schema/src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
